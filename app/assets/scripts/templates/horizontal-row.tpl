<div class="vis--row" id="<%= id %>">
	<span class="vis--row_title">
		<a href="<%= baseurl %>/companies/<%= href %>">
		<%= name %>
		</a>
	</span>
	<span class="vis--category_score">
		<span class="vis--category_score_value">
			<%= score %>
		</span>
	</span>
	<div class="vis--row_bars">
		<%= barContent %>
	</div>
</div>